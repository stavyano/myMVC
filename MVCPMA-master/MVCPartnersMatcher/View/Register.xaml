        <Label x:Name="label3" Content="תאריך לידה" HorizontalAlignment="Right" Margin="0,213,26,0" Width="78" Height="38" VerticalAlignment="Top" Grid.Column="1"/>
        <Label x:Name="label4" Content="מין" HorizontalAlignment="Right" Margin="0,0,26,93" Width="78" Height="29" VerticalAlignment="Bottom" Grid.Column="1"/>
        <Label x:Name="label6" Content="טלפון" HorizontalAlignment="Right" Height="29" Margin="0,0,26,48" VerticalAlignment="Bottom" Width="78" Grid.Column="1"/>
        <TextBox x:Name="FirstName" Height="29" Margin="21,150,133,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <TextBox x:Name="LastName" Height="29" Margin="21,184,133,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <DatePicker x:Name="date"  Margin="21,218,133,0" Height="33" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <TextBox x:Name="phone" Height="29" Margin="21,0,133,48" TextWrapping="Wrap" VerticalAlignment="Bottom" Grid.ColumnSpan="2"/>
        <Button x:Name="button" Content="הוסף תמונה" Height="26" Margin="0,0,17,1" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="111" Click="notAvailable" Grid.Column="1" />
        <Button x:Name="button1" Content="הרשמה" Height="30" Margin="34,0,173,1" VerticalAlignment="Bottom" Click="button1_Click" Grid.ColumnSpan="2"/>
        <StackPanel Margin="68,0,173,116" Height="34" VerticalAlignment="Bottom" Grid.ColumnSpan="2"/>
        <Label x:Name="label7" Content="אי מייל" HorizontalAlignment="Right" Height="29" Margin="0,57,26,0" VerticalAlignment="Top" Width="78" Grid.Column="1"/>
        <Label x:Name="label8" Content="סיסמה" HorizontalAlignment="Right" Height="29" Margin="0,91,26,0" VerticalAlignment="Top" Width="78" Grid.Column="1"/>
        <Label x:Name="label9" Content="סיסמה שוב" HorizontalAlignment="Right" Height="29" Margin="0,125,26,0" VerticalAlignment="Top" Width="78" Grid.Column="1"/>
        <TextBox x:Name="email" Height="29" Margin="21,57,133,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <TextBox x:Name="password" Height="29" Margin="21,91,133,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <TextBox x:Name="repassword" Height="29" Margin="21,125,133,0" TextWrapping="Wrap" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <ComboBox x:Name="gender" HorizontalAlignment="Left" Width="114" RenderTransformOrigin="0.509,1.308" Grid.ColumnSpan="2" Margin="68,267,0,95" />

    </Grid>
</Window>
