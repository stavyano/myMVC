<Window x:Class="MVCPartnersMatcher.View.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVCPartnersMatcher.View"
        mc:Ignorable="d"
        Title="Register" Height="438.462" Width="300">
    <Grid>
        
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="167*"/>
            <ColumnDefinition Width="188*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="309*"/>
            <RowDefinition Height="14*"/>
        </Grid.RowDefinitions>

        <Label x:Name="label" Content=" שם פרטי באנגלית" HorizontalAlignment="Right" Height="29" Margin="0,150,10,0" VerticalAlignment="Top" Width="111" Grid.Column="1"/>
